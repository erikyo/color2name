{
  "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.tsx?$",
  "globals": {
    "ts-jest": {
      "useESM": true
    }
  },
  "transformIgnorePatterns": ["/node_modules/"],
  "coverageThreshold": {
    "global": {
      "branches": 10,
      "functions": 10,
      "lines": 10,
      "statements": 10
    }
  },
  "collectCoverage": true,
}
